<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical">
        <Button
            android:id="@+id/btnappoint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Appoinments" />
        <Button
            android:id="@+id/btnaut"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="AUTH" />
        <Button
            android:id="@+id/btnEnroll"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Enroll" />
        <Button
            android:id="@+id/btnSign"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Sign" />

        <Button
            android:id="@+id/btnFing"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Finger" />
        <Button
            android:id="@+id/btnVideo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Videoconference" />

        <Button
            android:id="@+id/btnOther"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Others docs" />
        <Button
            android:id="@+id/ZTS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Tensor" />
        <Button
            android:id="@+id/FIR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/face_entity_resumen" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>